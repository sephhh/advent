{
    "compilerOptions": {
      "checkJs": false,
      "skipLibCheck": false,
      "noUnusedParameters": true,
      "strict": true,
      "strictFunctionTypes": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "noImplicitAny": true,
      "noUncheckedIndexedAccess": true,
      "declaration": false,
      "lib": ["es6", "dom"],
      "target": "es6",
      "module": "es6",
      "moduleResolution": "node",
    },
    "exclude": [
      "./node_modules/*",
      "**/node_modules/*",
    ],
  }
